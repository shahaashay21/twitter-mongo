<div class="col-xs-12 tweetpost no-padding">
	
	<!-- <span ng-if="orm == 0"> -->
		<div class="col-xs-12 no-padding show-tweetbox" ng-repeat="post in recentTweets">
			<span ng-if="!post.parentTweet">
				<div class="col-xs-1" ng-if="post.user.dp">
		    		<a href="/user/{{ post.tweet.user_id }}"><img ng-src="/img/{{ post.user.dp }}" class="img-rounded menu-img" style="margin: 0px; height: 48px; width:48px"></a>
				</div>
				<div class="col-xs-11 no-padding" style="margin-top: -1px; padding-left: 16px">
					<div class="col-xs-12">
						<a href="/user/{{ post.tweet.user_id }}"><span class="camel" style="color: #000"><b>{{ post.user.fname }} {{ post.user.lname }}</b></span></a>
						<span style="color: #8899a6">
							<span ng-if="post.user.tweet_handle">
							<a style="color: #8899a6" href="/user/{{ post.tweet.user_id }}">
								@{{ post.user.tweet_handle }}
							</a>
							</span>
							 - {{ post.tweet.time }}
						</span>
					</div>
					<div class="col-xs-12">
						<span ng-if="post.tweet.tweet.length > 0">
							<span style="color: #292f33" ng-bind-html="post.tweet.tweet | unsafe">{{ post.tweet.tweet }}</span>
						<span>
					</div>
					<div class="col-xs-12" ng-if="post.tweet.img_url.length > 0">
						<img ng-src="/img/{{ post.tweet.img_url }}" class="img-rounded menu-img" style="margin: 0px; margin-top: 10px; height: 100%; width: 100%; padding-bottom:15px">
					</div>
				</div>
				<div class="col-xs-12" style="color: #aab8c2">
					<div class="col-xs-offset-1 col-xs-2">
						<i class="fa fa-retweet retweet" ng-show="post.retweet" ng-click="retweet(userid,post.tweet.user_id,post.tweet.id,0,$index)" style="color: #19cf86"></i>
						<i class="fa fa-retweet retweet" ng-hide="post.retweet" ng-click="retweet(userid,post.tweet.user_id,post.tweet.id,0,$index)"></i>
						{{ post.tweet.retweets }} 
					</div>
					<div class="col-xs-2">
							<i class="fa fa-heart like" ng-show="post.like" style="color:#e81c4f" ng-click="like(post.tweet.id,post.userlike,$index,0)"></i>
							<i class="fa fa-heart like" ng-hide="post.like" ng-click="like(post.tweet.id,post.userlike,$index,0)"></i>
						<span>{{ post.tweet.likes }}</span>
					</div>
					<div class="col-xs-2" ng-if="userid == post.tweet.user_id">
						<div class="dropdown">
						    <i class="fa fa-ellipsis-h option dropdown-toggle" type="button" data-toggle="dropdown"></i>
						    <ul class="dropdown-menu">
						      <li><a href="" ng-click="deleteTweet(post.tweet.id)">Delete</a></li>
						    </ul>
					    </div>
					</div>
				</div>
			</span>
			<span ng-if="post.parentTweet">
				<div class="col-xs-12" style="padding-left: 50px; padding-bottom: 10px">
					<i class="fa fa-retweet" style="color: #19cf86"></i>
					<span class="camel" style="color: #8899a6"> {{ post.user.fname }} {{ post.user.lname }} Retweeted</span>
				</div>
				<div class="col-xs-1" ng-if="post.parentUser.dp">
		    		<a href="/user/{{ post.parentTweet.user_id }}"><img ng-src="/img/{{ post.parentUser.dp }}" class="img-rounded menu-img" style="margin: 0px; height: 48px; width:48px"></a>
				</div>
				<div class="col-xs-11 no-padding" style="margin-top: -1px; padding-left: 16px">
					<div class="col-xs-12">
						<a href="/user/{{ post.parentTweet.user_id }}"><span class="camel" style="color: #000"><b>{{ post.parentUser.fname }} {{ post.parentUser.lname }}</b></span></a>
						<span style="color: #8899a6">
							<span ng-if="post.tweet_handle">
							<a style="color: #8899a6" href="/user/{{ post.parentTweet.user_id }}">
								@{{ post.parentUser.tweet_handle }}
							</a>
							</span>
							 - {{ post.parentTweet.time }}
						</span>
					</div>
					<div class="col-xs-12">
						<span ng-if="post.parentTweet.tweet != null">
							<span style="color: #292f33" ng-bind-html="post.parentTweet.tweet | unsafe">{{ post.parentTweet.tweet }}</span>
						<span>
					</div>
					<div class="col-xs-12" ng-if="post.parentTweet.img_url.length > 0">
						<img ng-src="/img/{{ post.parentTweet.img_url }}" class="img-rounded menu-img" style="margin: 0px; margin-top: 10px; height: 100%; width: 100%; padding-bottom:15px">
					</div>
				</div>
				<div class="col-xs-12" style="color: #aab8c2">
					<div class="col-xs-offset-1 col-xs-2">
						<i class="fa fa-retweet retweet" ng-show="post.retweet" ng-click="retweet(userid,post.parentTweet.user_id,post.parentTweet.id,1,$index)" style="color: #19cf86"></i>
						<i class="fa fa-retweet retweet" ng-hide="post.retweet" ng-click="retweet(userid,post.parentTweet.user_id,post.parentTweet.id,1,$index)"></i>
						{{ post.parentTweet.retweets }} 
					</div>
					<div class="col-xs-2">
							<i class="fa fa-heart like" ng-show="post.like" style="color:#e81c4f" ng-click="like(post.parentTweet.id,post.retweet_liked,$index,1)"></i>
							<i class="fa fa-heart like" ng-hide="post.like" ng-click="like(post.parentTweet.id,post.retweet_liked,$index,1)"></i>
						<span>{{ post.parentTweet.likes }}</span>
					</div>
					<!--<div class="col-xs-2" ng-if="user == post.user_id">
						<div class="dropdown">
						    <i class="fa fa-ellipsis-h option dropdown-toggle" type="button" data-toggle="dropdown"></i>
						    <ul class="dropdown-menu">
						      <li><a href="" ng-click="deleteTweet(post.id)">Delete</a></li>
						    </ul>
					    </div>
					</div>-->
				</div>
			</span>
		</div>
	<!-- </span> -->
	
	<span ng-if="orm == 1">
		<div class="col-xs-12 no-padding show-tweetbox" ng-repeat="post in tweets">
			<div class="col-xs-1" ng-if="post.user.dp">
	    		<img ng-src="/img/{{ post.user.dp }}" class="img-rounded menu-img" style="margin: 0px; height: 48px; width:48px">
			</div>
			<div class="col-xs-11 no-padding" style="margin-top: -1px; padding-left: 16px">
				<div class="col-xs-12">
					<span class="camel" style="color: #000"><b>{{ post.user.fname }} {{ post.user.lname }}</b></span>
					<span style="color: #8899a6">@{{ post.user.tweet_handle }} - {{ post.time }}</span>
				</div>
				<div class="col-xs-12">
					<span style="color: #292f33" ng-bind-html="post.tweet | unsafe">{{ post.tweet }}</span>
				</div>
				<div class="col-xs-12" ng-if="post.img_url != null">
					<img ng-src="/img/{{ post.img_url }}" class="img-rounded menu-img" style="margin: 0px; margin-top: 10px; height: 100%; width: 100%; padding-bottom:15px">
				</div>
			</div>
			<div class="col-xs-12" style="color: #aab8c2">
				<div class="col-xs-offset-1 col-xs-2">
					<i class="fa fa-retweet retweet"></i> 12 
				</div>
				<div class="col-xs-2">
					<div ng-if="post.userlike == 1">
						<i class="fa fa-heart like" style="color:#e81c4f" ng-click="like(post.id,$index)"></i>
					</div>
					<div ng-if="post.userlike == 0">
						<i class="fa fa-heart like" ng-click="like(post.id,$index)"></i>
					</div>
					<span ng-repeat="userlike in tweetlike">
						<span ng-if="userlike.tweet_id == post.id">
							<span>{{ userlike.likes }}</span>
						</span>
					</span>
				</div>
				<div class="col-xs-2" ng-if="user == post.user_id">
					<div class="dropdown">
					    <i class="fa fa-ellipsis-h option dropdown-toggle" type="button" data-toggle="dropdown"></i>
					    <ul class="dropdown-menu">
					      <li><a href="" ng-click="deleteTweet(post.id)">Delete</a></li>
					    </ul>
				    </div>
				</div>
			</div>
		</div>
	<span>
	
	
</div>